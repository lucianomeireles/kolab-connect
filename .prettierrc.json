{
  "printWidth": 80,
  "trailingComma": "none",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "useSpaces": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "requireConfig": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "endOfLine": "auto"
}
